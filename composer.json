{
    "name": "phlib/csv",
    "description": "A CSV parsing library; Prevents out of memory errors when parsing large files without a closing string delimiter",
    "keywords": ["csv"],
    "license": "LGPL-3.0",
    "require": {
        "php": "^7.1",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "guzzlehttp/psr7": "^1"
    },
    "autoload": {
        "psr-4": {
            "Phlib\\Csv\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phlib\\Csv\\Tests\\": "tests/"
        }
    }
}
